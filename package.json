{
  "name": "using-openapi-in-testing",
  "version": "1.0.0",
  "description": "Code for the talk 'Using OpenAPI in Testing'",
  "scripts": {
    "dev:all": "concurrently \"npm run mocks:watch\" \"npm run dev\"",
    "dev": "nuxt",
    "mocks": "prism mock -d ./data/petstore.yml",
    "mocks:watch": "nodemon --exec npm run mocks -w ./data/petstore.yml",
    "test:e2e": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrtnvh/using-openapi-in-testing.git"
  },
  "keywords": [
    "testing",
    "nuxt",
    "vue",
    "e2e",
    "unit",
    "openapi",
    "prism"
  ],
  "author": {
    "name": "Maarten Van Hoof",
    "url": "https://mrtnvh.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrtnvh/using-openapi-in-testing/issues"
  },
  "homepage": "https://github.com/mrtnvh/using-openapi-in-testing#readme",
  "dependencies": {
    "@nuxtjs/axios": "^5.6.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.10.1"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^3.1.1",
    "@stoplight/prism-core": "^3.1.1",
    "@stoplight/prism-http": "^3.1.1",
    "@stoplight/prism-http-server": "^3.1.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@wemake-services/stylelint-config-scss": "^0.1.4",
    "autoprefixer": "^9.6.5",
    "concurrently": "^5.0.0",
    "cssnano": "^4.1.10",
    "cypress": "^3.4.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-nuxt": "^0.1.5",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-gridsome": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.3",
    "postcss-import": "^12.0.1",
    "postcss-media-variables": "^2.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-nested-ancestors": "^2.0.0",
